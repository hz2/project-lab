{
  "name": "project-lab-ts",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "antd": "^4.18.2",
    "connected-react-router": "^6.9.2",
    "http-proxy-middleware": "^2.0.1",
    "jszip": "^3.7.1",
    "qs": "^6.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-transition-group": "^4.4.2",
    "sanitize.css": "^13.0.0",
    "svgo": "^2.8.0",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.3"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "craco-less": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "pub.css": "^0.0.32",
    "react-app-rewire-alias": "^1.1.7"
  },
  "scripts": {
    "dev": "craco start",
    "start": "craco start",
    "build": "set GENERATE_SOURCEMAP=false && craco build",
    "lint": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
