{
  "name": "project-lab-ts",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "antd": "^4.17.3",
    "connected-react-router": "^6.9.1",
    "history": "^4.7.2",
    "http-proxy-middleware": "^0.19.2",
    "jszip": "^3.7.0",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "sanitize.css": "^10.0.0",
    "svgo": "^2.8.0",
    "typescript": "^4.5.2",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "craco-less": "^1.20.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^1.15.2",
    "pub.css": "^0.0.27",
    "react-app-rewire-alias": "^1.1.5"
  },
  "scripts": {
    "dev": "craco start",
    "start": "craco start",
    "build": "set GENERATE_SOURCEMAP=false && craco build",
    "lint": "prettier --single-quote --write src/**/*.{js,jsx,json,css}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
